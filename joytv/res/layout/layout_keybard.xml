<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:layout_width="fill_parent"
    android:layout_height="fill_parent"
     xmlns:android="http://schemas.android.com/apk/res/android">
<LinearLayout android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_alignParentBottom="true"
    android:background="@drawable/bg_keyboard"
    android:layout_marginLeft="235dp"
    android:layout_marginRight="235dp"
    android:paddingRight="10dp"
    android:paddingTop="10dp"
    android:orientation="vertical" >
    <LinearLayout android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:orientation="horizontal">
        <LinearLayout android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="3"
            android:orientation="horizontal">
            <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:id="@+id/key_a"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:text="A"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:id="@+id/key_b"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:text="B"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:id="@+id/key_c"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:text="C"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:id="@+id/key_d"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:text="D"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:id="@+id/key_e"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:text="E"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:id="@+id/key_f"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:text="F"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:id="@+id/key_g"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:text="G"/>
        </LinearLayout>
        <LinearLayout android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal"
            android:layout_weight="7">
            <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:id="@+id/key_1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:text="1"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:id="@+id/key_2"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:text="2"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:id="@+id/key_3"
            android:nextFocusRight="@id/key_a"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:text="3"/>
        </LinearLayout>
    </LinearLayout>
	<LinearLayout android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:orientation="horizontal"
        android:layout_marginTop="5dp">
         <LinearLayout android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="3"
            android:orientation="horizontal">
        	 <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:id="@+id/key_h"
            android:text="H"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:id="@+id/key_i"
            android:text="I"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:id="@+id/key_j"
            android:text="J"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:id="@+id/key_k"
            android:text="K"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:id="@+id/key_l"
            android:text="L"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:id="@+id/key_m"
            android:text="M"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:id="@+id/key_n"
            android:text="N"/>
        </LinearLayout>
        <LinearLayout android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal"
            android:layout_weight="7">
        	<Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:id="@+id/key_4"
            android:text="4"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:id="@+id/key_5"
            android:text="5"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:id="@+id/key_6"
            android:nextFocusRight="@id/key_h"
            android:text="6"/>
         </LinearLayout>
    </LinearLayout>
    <LinearLayout android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:layout_marginTop="5dp">
        <LinearLayout android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="3"
            android:orientation="horizontal">
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:id="@+id/key_o"
            android:text="O"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:id="@+id/key_p"
            android:text="P"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
