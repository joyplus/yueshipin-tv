        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:id="@+id/key_x"
            android:text="X"/>
        <Button android:layout_width="fill_parent"
