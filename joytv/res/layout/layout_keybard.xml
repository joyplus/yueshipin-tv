        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:id="@+id/key_z"
            android:text="Z"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:background="@drawable/bg_button_keyboard_search_selector"
            android:layout_marginLeft="5dp"
            android:textColor="@color/keyboard_text_color_selector"
            android:textSize="20dp"
            android:id="@+id/key_search"
            android:paddingBottom="2dp"
            android:text="搜索"/>
        </LinearLayout>
        <LinearLayout android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="7"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal">
        <!-- <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:background="@drawable/bg_button_keyboard_selector"
            android:textColor="@color/text_color_selector"
            android:layout_marginLeft="5dp"
            android:textSize="30dp"
            android:id="@+id/key_doute"
            android:paddingBottom="2dp"
            android:text="·"/> -->
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:layout_marginLeft="5dp"
            style="@style/keyboardButton"
            android:id="@+id/key_0"
            android:text="0"/>
        <Button android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="2"
            android:layout_marginLeft="5dp"
            android:nextFocusRight="@id/key_u"
            android:background="@drawable/bg_button_keyboard_cancle_selector"
            android:id="@+id/key_cancle"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
</RelativeLayout>