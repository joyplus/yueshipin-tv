<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

	   <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                    android:startColor="#ff202932"
                    android:centerColor="#ff2e3843"
                    android:centerY="0.75"
                    android:endColor="#ff2e3843"
                    android:angle="270"
            />
        </shape>
    </item>
    
	 <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                        android:startColor="#ff202932"
                        android:centerColor="#ff2e3843"
                        android:centerY="0.75"
                        android:endColor="#ff2e3843"
                        android:angle="270"
                />
            </shape>
        </clip>
    </item>
	<item android:id="@+android:id/progress" android:drawable="@drawable/player_time_jindu_x" />

</layer-list>